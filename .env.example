# ==================================================
# CONFIGURAÇÕES BÁSICAS DO DJANGO
# ==================================================

# Módulo de configurações Django (production, development, etc.)
# DJANGO_SETTINGS_MODULE=gestaoestoque.settings

# Debug mode (True/False)
# DJANGO_SETTINGS_DEBUG=False

# Chave secreta do Django (necessária para produção)
# SECRET_KEY=your-secret-key-here-generate-a-new-one-for-production
# DJANGO_SECRET_KEY=<your-secret-key>

# Versão da aplicação
# APP_VERSION=1.0.0

# Ambiente (development, test, production, etc.)
# AMBIENTE=production

# Hosts permitidos (separados por vírgula)
# DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1

# Primary host (usado para URLs absolutas)
# PRIMARY_HOST=example.com

# Nível de log do Django
# DJANGO_LOG_LEVEL=INFO

# Diretório raiz para arquivos de media
# DJANGO_MEDIA_ROOT=/path/to/media

# Diretório raiz para arquivos estáticos (produção)
# DJANGO_STATIC_ROOT=/path/to/static

# ==================================================
# CONFIGURAÇÕES DE BANCO DE DADOS
# ==================================================

# Engine do banco de dados
# DATABASE_ENGINE=django.db.backends.postgresql

# Host do banco de dados
# DATABASE_HOST=localhost

# Porta do banco de dados
# DATABASE_PORT=5432

# Nome do banco de dados
# DATABASE_NAME=gestao_estoque

# Usuário do banco de dados
# DATABASE_USERNAME=postgres

# Senha do banco de dados
# DATABASE_PASSWORD=<your-database-password>

# URL completa do banco de dados (alternativa às configurações individuais)
# DATABASE_URL=postgresql://user:password@host:port/dbname

# ==================================================
# CONFIGURAÇÕES DE REDIS/CACHE
# ==================================================

# URL do Redis
# REDIS_URL=redis://localhost:6379

# URL do Redis com TLS
# REDIS_TLS_URL=rediss://user:password@host:port

# ==================================================
# CONFIGURAÇÕES DE EMAIL
# ==================================================

# Backend de email
# EMAIL_BACKEND=django.core.mail.backends.smtp.EmailBackend

# Servidor SMTP
# EMAIL_HOST=gerencia.webmail.pb.gov.br

# Porta do servidor SMTP
# EMAIL_PORT=587

# Usuário do email
# EMAIL_HOST_USER=naoresponda_sites@codata.pb.gov.br

# Senha do email
# EMAIL_HOST_PASSWORD=<your-email-password>

# Usar TLS
# EMAIL_USE_TLS=True

# Usar SSL
# EMAIL_USE_SSL=False

# Email padrão para envio
# DEFAULT_FROM_EMAIL=naoresponda_sites@codata.pb.gov.br

# Nome padrão para envio
# DEFAULT_FROM_NAME=Não responda Secretaria de Educação [TST]

# ==================================================
# CONFIGURAÇÕES DO WAGTAIL
# ==================================================

# URL base do admin do Wagtail
# WAGTAILADMIN_BASE_URL=https://qa.portais-secretaria-da-educacao.rke.codatahml.pb.gov.br

# ==================================================
# CONFIGURAÇÕES DE ELASTICSEARCH
# ==================================================

# Endpoint do Elasticsearch
# ELASTICSEARCH_ENDPOINT=localhost

# Porta do Elasticsearch
# ELASTICSEARCH_PORT=9200

# Usar SSL para Elasticsearch
# ELASTICSEARCH_USE_SSL=off

# Verificar certificados SSL
# ELASTICSEARCH_VERIFY_CERTS=off

# ==================================================
# CONFIGURAÇÕES DE STORAGE AWS S3
# ==================================================

# Chave de acesso AWS
# AWS_ACCESS_KEY_ID=<your-aws-access-key>

# Chave secreta AWS
# AWS_SECRET_ACCESS_KEY=<your-aws-secret-key>

# Região AWS
# AWS_REGION=us-east-1

# Token de sessão AWS (opcional)
# AWS_SESSION_TOKEN=<your-session-token>

# Nome do bucket S3
# AWS_STORAGE_BUCKET_NAME=<your-bucket-name>

# Domínio personalizado S3
# AWS_S3_CUSTOM_DOMAIN=<your-custom-domain>

# Região S3
# AWS_S3_REGION_NAME=us-east-1

# ==================================================
# CONFIGURAÇÕES DE STORAGE GOOGLE CLOUD
# ==================================================

# Nome do bucket Google Cloud
# GS_BUCKET_NAME=<your-bucket-name>

# ID do projeto Google Cloud
# GS_PROJECT_ID=<your-project-id>

# ==================================================
# CONFIGURAÇÕES DE APIS EXTERNAS
# ==================================================

# API do Metabase
# METABASE_API_KEY=<your-metabase-api-key>
# METABASE_API_URL=https://metabase.codata.pb.gov.br/api/card/

# APIs de Integração
# API_EXTERNA_URL=https://api.exemplo.com/
# SISTEMA_EXTERNO_URL=https://sistema.exemplo.com/

# ==================================================
# CONFIGURAÇÕES DE SSO/KEYCLOAK
# ==================================================

# Habilitar login SSO
# HABILITAR_SSO_LOGIN=False

# URL base do Keycloak
# KEYCLOAK_BASE_URL=https://homolog.sso.codata.pb.gov.br/auth

# Realm do Keycloak
# KEYCLOAK_REALM=desenvolvimento

# Configurações do provedor social
# SOCIALACCOUNT_PROVIDER_ID=codata-sso-dev-server
# SOCIALACCOUNT_NAME=CODATA SSO Server

# Credenciais do cliente Keycloak
# KEYCLOAK_CLIENT_ID=dev-client-secret
# KEYCLOAK_SECRET=<your-keycloak-client-secret>

# URLs do servidor Keycloak
# KEYCLOAK_SERVER_URL=https://homolog.sso.codata.pb.gov.br/auth/realms/desenvolvimento/.well-known/openid-configuration
# KEYCLOAK_SERVER_URL_LOGOUT=https://homolog.sso.codata.pb.gov.br/auth/realms/desenvolvimento/protocol/openid-connect/logout

# ==================================================
# CONFIGURAÇÕES DE FRONTEND CACHE (CLOUDFLARE)
# ==================================================

# Token de API Cloudflare (opção 1 - token com acesso restrito)
# FRONTEND_CACHE_CLOUDFLARE_BEARER_TOKEN=<your-bearer-token>

# Credenciais Cloudflare (opção 2 - chave de API geral)
# FRONTEND_CACHE_CLOUDFLARE_TOKEN=<your-api-token>
# FRONTEND_CACHE_CLOUDFLARE_EMAIL=<your-email>

# ID da zona Cloudflare
# FRONTEND_CACHE_CLOUDFLARE_ZONEID=<your-zone-id>

# ==================================================
# CONFIGURAÇÕES DE BASIC AUTH
# ==================================================

# Habilitar autenticação básica
# BASIC_AUTH_ENABLED=false

# Credenciais de autenticação básica
# BASIC_AUTH_LOGIN=wagtail
# BASIC_AUTH_PASSWORD=wagtail

# Hosts liberados da autenticação básica (separados por vírgula)
# BASIC_AUTH_WHITELISTED_HTTP_HOSTS=localhost,127.0.0.1

# ==================================================
# CONFIGURAÇÕES DE CONTENT SECURITY POLICY (CSP)
# ==================================================

# Política padrão CSP
# CSP_DEFAULT_SRC='self'

# Scripts permitidos
# CSP_SCRIPT_SRC='self','unsafe-inline'

# Estilos permitidos
# CSP_STYLE_SRC='self','unsafe-inline'

# Imagens permitidas
# CSP_IMG_SRC='self',data:

# Conexões permitidas
# CSP_CONNECT_SRC='self'

# Fontes permitidas
# CSP_FONT_SRC='self'

# Base URI permitida
# CSP_BASE_URI='self'

# Objetos permitidos
# CSP_OBJECT_SRC='none'

# ==================================================
# CONFIGURAÇÕES DE FEATURES/FUNCIONALIDADES
# ==================================================


# Sistema provedor de dados
# SISTEMA_PROVEDOR_DADOS=False

# API de conteúdo agrupado
# API_CONTEUDO_AGRUPADO=False

# Variável de ambiente para CI/CD
# CI=false

